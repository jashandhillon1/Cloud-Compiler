<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>usersignupresponse</servlet-name>
        <servlet-class>usersignupresponse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userloginresponse</servlet-name>
        <servlet-class>userloginresponse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userlogout</servlet-name>
        <servlet-class>userlogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userlogout1</servlet-name>
        <servlet-class>userlogout1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userchangepassword</servlet-name>
        <servlet-class>userchangepassword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchresponse</servlet-name>
        <servlet-class>searchresponse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>usersignupresponse</servlet-name>
        <url-pattern>/usersignupresponse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userloginresponse</servlet-name>
        <url-pattern>/userloginresponse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userlogout</servlet-name>
        <url-pattern>/userlogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userlogout1</servlet-name>
        <url-pattern>/userlogout1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userchangepassword</servlet-name>
        <url-pattern>/userchangepassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchresponse</servlet-name>
        <url-pattern>/searchresponse</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
